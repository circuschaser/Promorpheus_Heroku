<% provide(:title, "Archived Setlists") %>

<div class="notice_br">
  <% flash.each do |key, value| %>
    <%= content_tag(:div, value, class: "alert-temp alert fade in alert-#{key}") %>
  <% end %>
</div>

<div id="content_main">

  <div class="setlist_trail">
    <%= link_to "ALL MY SETLISTS", setlists_path %>
    <%= image_tag("breadcrumb.png", id: "breadcrumb-sm") %>
  </div>

  <div id="addbutton_br">
    <%= link_to "NEW SETLIST", new_setlist_path, id: 'addtrack', :class => 'btn-small btn-small-grn' %>
  </div>

  <div class="setlist_title">
    <div class="mainlink">
      <h4><%= link_to "ARCHIVED SETLISTS", archived_setlists_path %></h4>
    </div>
  </div>


  <div class="greywrap_top">
  </div>
  <div class="greywrap">

    <div class="content_head">
  
      <div class='pagination'>
        <%= will_paginate @setlists.archived %>
      </div>
    </div>

    <div class="content_body">

      <%= render "setlists/archived" %>

    </div>

  </div>
</div>

