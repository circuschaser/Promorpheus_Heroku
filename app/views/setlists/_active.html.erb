  <div id="item_counter">
    <%= page_entries_info @setlists %>
    <p id="new">
      <label>NEW SETLIST</label>
      <%= link_to image_tag("icon_add.png"), new_setlist_path, class: "icon_add" %>
    </p><br/>
  </div>

<% if @setlists.active.size == 0 %>

  <div class="empty_track">
      <p>No active setlists found.</p>
  </div>

<% else %>

  <div>

      <%= render 'setlists/display_act' %>

  </div>

<% end %>

