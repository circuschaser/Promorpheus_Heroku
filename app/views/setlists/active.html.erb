    <%= link_to "ALL MY SETLISTS", setlists_path %>
    <%= image_tag("breadcrumb.png", id: "breadcrumb-sm") %>
  </div>
  <div class="setlist_title">
    <div id="search_form">
      <%= form_tag setlists_path, method: 'get' do %>
      <%= text_field_tag :search, params[:search], placeholder: "Search your setlists" %>
      <% end %>
    </div>
