      <h3><%= link_to "ACTIVE SETLISTS", active_setlists_path %></h3>
    </div>
  </div>


  <div class="greywrap_top">
  </div>
  <div class="greywrap">

    <div class="content_head">
  
      <div class='pagination'>
        <%= will_paginate @setlists.active %>
      </div>
    </div>

    <div class="content_body">

      <%= render "setlists/active" %>

    </div>

  </div>
